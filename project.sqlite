SELECT *
FROM orders
LIMIT 10;

SELECT DISTINCT order_date
FROM orders;

SELECT special_instructions
FROM orders
LIMIT 20;

SELECT special_instructions
FROM order
WHERE special_instruction IS NOT NULL;

SELECT special_instructions
FROM order
WHERE special_instruction IS NOT NULL
ORDER BY special_instructions ASC;

SELECT special_instruction
FROM order
WHERE special_instruction LIKE '%sauce%';

SELECT special_instruction
FROM order
WHERE special_instruction LIKE '%door%';

SELECT special_instruction
FROM order
WHERE special_instruction LIKE '%box%'

SELECT id AS #, 
special_instructions AS 'Notes'
FROM order
WHERE special_instructions LIKE '%box%';






